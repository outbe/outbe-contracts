          "commit_id": {
            "type": "string"
          },
          "contracts": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/ContractInfo"
            }
          },
          "is_latest": {
            "description": "identifies whenever to mark this deployment as \"latest\"",
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "ExecuteMsg": {
        "oneOf": [
          {
            "description": "Transfers ownership, revokes the old owner and grants the new one",
            "type": "object",
            "required": [
              "transfer_ownership"
            ],
            "properties": {
              "transfer_ownership": {
                "type": "object",
                "required": [
                  "new_owner"
                ],
                "properties": {
                  "new_owner": {
                    "$ref": "#/definitions/Addr"
                  }
                },
                "additionalProperties": false
              }
            },
            "additionalProperties": false
          },
          {
            "description": "Pauses the contract",
            "type": "object",
            "required": [
              "update_pause"
            ],
            "properties": {
              "update_pause": {
                "type": "object",
                "required": [
                  "pause"
                ],
                "properties": {
                  "pause": {
                    "type": "boolean"
                  }
                },
                "additionalProperties": false
              }
            },
            "additionalProperties": false
          }
        ]
      }
    }
  },
